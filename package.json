{
  "name": "red-tetris",
  "version": "1.0.0",
  "description": "Projet red-tetris de 42, stack js",
  "main": "index.js",
  "scripts": {
    "dev-client": "webpack-dev-server --colors --hot --inline --host 0.0.0.0 --port 8080",
    "dev-server": "babel-watch src/server/start.js",
    "build": "NODE_ENV=production webpack --progress --colors",
    "start": "NODE_ENV=production node src/server/start.js",
    "eslint": "eslint src",
    "test": "NODE_ENV=test mocha --require @babel/register --require ./test/helpers.js --require ./test/dom.js --reporter spec \"src/**/*.test.js\"",
    "coverage": "NODE_ENV=test nyc --include src --all -r text -r lcov --require @babel/register npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/eloi-duwer/red-tetris.git"
  },
  "author": "eloi-duwer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eloi-duwer/red-tetris/issues"
  },
  "homepage": "https://github.com/eloi-duwer/red-tetris#readme",
  "dependencies": {
    "react": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-watch": "^7.0.0",
    "chai": "^4.2.0",
    "css-loader": "^3.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "install": "^0.13.0",
    "jsdom": "^15.2.1",
    "mocha": "^7.0.0",
    "npm": "^6.13.6",
    "nyc": "^15.0.0",
    "prop-types": "^15.7.2",
    "react-dom": "^16.12.0",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "nyc": {
    "exclude": [
      "src/**/*.test.js",
      "build"
    ],
    "require": [
      "babel-loader"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
